{
  "expo": {
    "name": "expo-redux-hooks",
    "slug": "expo-redux-hooks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/app_icon.png",
    "splash": {
      "image": "./app/assets/images/splash_image.png",
      "resizeMode": "cover",
      "backgroundColor": "#ff0000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/app_icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/images/app_icon.png"
    }
  }
}
